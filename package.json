{
  "name": "poka",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest run",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 -h localhost",
    "server": "node server/index.mjs",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@vueuse/core": "^8.3.1",
    "fastify": "^3.28.0",
    "fastify-cors": "^6.0.3",
    "fastify-static": "^4.6.1",
    "pinia": "^2.0.13",
    "pino": "^7.10.0",
    "pino-pretty": "^7.6.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rushstack/eslint-patch": "^1.1.3",
    "@storybook/addon-actions": "6.5.0-alpha.64",
    "@storybook/addon-essentials": "6.5.0-alpha.64",
    "@storybook/addon-links": "6.5.0-alpha.64",
    "@storybook/addon-storysource": "6.5.0-alpha.64",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/vue3": "6.5.0-alpha.64",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.25",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.14.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "^2.55.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "typescript": "~4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-windicss": "^1.8.4",
    "vitest": "^0.9.4",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.9",
    "windicss": "^3.5.1"
  }
